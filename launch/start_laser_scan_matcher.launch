<!--
Example launch file: launches the scan matcher with pre-recorded data
-->

<launch>


<!--   <node name="pose2D_2_posestamped" pkg="slam" type="pose2D_2_posestamped.py" respawn="true" output="screen"/> -->


  <remap from="/vel" to="/cmd_vel"/>

  <node pkg="laser_scan_matcher" type="laser_scan_matcher_node"
    name="laser_scan_matcher_node_slam" output="screen">
    <param name="fixed_frame"       value="map"/>
    <param name="base_frame"        value="base_link"/>

    <param name="use_vel"           value="true"/>

    <param name="publish_tf"              value="false"/>
    <param name="publish_pose_stamped"    value="true"/>

    <param name="max_iterations" value="10"/>
    <param name="max_correspondence_dist" value="0.3"/>
    <param name="max_max_angular_correction_deg" value="45.0"/>
    <param name="max_linear_correction" value="0.5"/>
<!--     <param name="epsilon_xy" value="10"/>
    <param name="epsilon_theta" value="10"/> -->
  </node>


  <node pkg="rviz" type="rviz" name="rviz_slam"
        args="-d $(find slam)/rviz/xwam.rviz"/>

</launch>
